{
  "name": "@orama/plugin-nextra",
  "version": "2.0.18",
  "description": "Nextra plugin for local search powered by orama",
  "keywords": [
    "orama",
    "nextra"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "type": "module",
  "bugs": {
    "url": "https://github.com/oramasearch/orama/issues"
  },
  "homepage": "https://github.com/oramasearch/orama#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oramasearch/orama.git"
  },
  "sideEffects": false,
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "swc --delete-dir-on-start --extensions .ts,.tsx,.cts -d dist src && cp ./node_modules/@orama/searchbox/dist/index.css dist/index.css",
    "a": "tsc -p . --emitDeclarationOnly",
    "lint": "eslint src --ext .js,.ts,.cts"
  },
  "dependencies": {
    "@orama/orama": "^2.0.18",
    "@orama/searchbox": "1.0.0-rc27",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "*.{ts, tsx}": "eslint ./src --cache --fix"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.65",
    "@swc/core": "^1.5.5",
    "typescript": "^5.4.5"
  }
}
