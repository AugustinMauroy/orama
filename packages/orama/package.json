{
  "name": "@orama/orama",
  "version": "2.0.9",
  "type": "module",
  "description": "Next generation full-text and vector search engine, written in TypeScript",
  "sideEffects": false,
  "main": "./dist/script/index.js",
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/script/index.js"
    },
    "./internals": {
      "types": "./dist/esm/internals.d.ts",
      "import": "./dist/esm/internals.js",
      "require": "./dist/script/internals.js"
    },
    "./components": {
      "types": "./dist/esm/components.d.ts",
      "import": "./dist/esm/components.js",
      "require": "./dist/script/components.js"
    },
    "./trees": {
      "types": "./dist/esm/trees.d.ts",
      "import": "./dist/esm/trees.js",
      "require": "./dist/script/trees.js"
    }
  },
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist-esm",
    "dist-cjs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oramasearch/orama"
  },
  "bugs": {
    "url": "https://github.com/oramasearch/orama"
  },
  "scripts": {
    "prebuild": "npm run lint",
    "build": "deno run -A dnt.ts",
    "--build": "npm run build:babel:esm && npm run build:babel:cjs && npm run build:types:esm && npm run build:types:cjs",
    "build:babel:esm": "rimraf dist-esm && babel src --extensions \".ts\" --out-dir dist-esm",
    "build:babel:cjs": "rimraf dist-cjs && babel src/cjs --extensions \".ts,.cts\" --out-dir dist-cjs",
    "build:types:esm": "tsc -p . --emitDeclarationOnly --declarationDir dist-esm",
    "build:types:cjs": "tsc -p tsconfig.cjs.json --emitDeclarationOnly --declarationDir dist-cjs",
    "lint": "eslint src tests --ext .js,.ts,.cts",
    "test": "c8 -c tests/config/c8-local.json tap tests/*.test.ts",
    "test:ci": "c8 -c tests/config/c8-ci.json tap --no-color tests/*.test.ts",
    "test:smoke": "tap tests/smoke/*.test.ts",
    "test:types": "tsc --emitDeclarationOnly; tsd --files tests/type/",
    "ci": "npm run build && npm run test:ci && npm run test:types"
  },
  "keywords": [
    "full-text search",
    "vector search",
    "vector database",
    "vectors",
    "search",
    "fuzzy search",
    "typo-tolerant search",
    "full-text",
    "answer engine"
  ],
  "author": {
    "name": "Michele Riva",
    "email": "michele.riva@oramasearch.com",
    "url": "https://github.com/MicheleRiva",
    "author": true
  },
  "contributors": [
    {
      "name": "Tommaso Allevi",
      "email": "tommaso.allevi@oramasearch.com",
      "url": "https://github.com/allevo"
    },
    {
      "name": "Paolo Insogna",
      "email": "paolo.insogna@oramasearch.com",
      "url": "http://cowtech.it",
      "author": true
    }
  ],
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/plugin-transform-export-namespace-from": "^7.23.4",
    "@babel/preset-typescript": "^7.23.3",
    "@orama/stemmers": "workspace:*",
    "@orama/stopwords": "workspace:*",
    "@playwright/test": "^1.29.2",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@types/node": "^20.9.0",
    "@types/tap": "^15.0.7",
    "auto-changelog": "^2.4.0",
    "c8": "^7.12.0",
    "commitizen": "^4.2.6",
    "glob": "^9.2.3",
    "jscodeshift": "^0.15.1",
    "prettier": "^2.8.1",
    "rimraf": "^5.0.5",
    "tap": "^18.6.1",
    "tap-mocha-reporter": "^5.0.3",
    "tape": "^5.6.1",
    "tcompare": "^6.0.0",
    "tsd": "^0.29.0",
    "tsx": "^3.12.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "tap": {
    "extends": "tests/config/tap.yml"
  }
}
